{
  "name": "Eyestrain reliever 3000",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "No eyestrain no gain",
  "homepage_url": "http://uva.nl",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_popup": "src/popup/popup.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "webNavigation",
    "activeTab",
    "geolocation",
    "https://*/*"
  ]
}
